{
  "compilerOptions": {
    "baseUrl": "src",
    "rootDir": "src",
    "module": "CommonJS",
    "target": "ES2015",
    "outDir": "dist",
    "tsBuildInfoFile": "dist/tsconfig.tsbuildinfo",
    "lib": [
      "es2016",
      "es2019",
      "ES2020",
      "dom"
    ],
    "moduleResolution": "node",
    "declaration": true,
    "declarationMap": false,
    "esModuleInterop": true,
    "incremental": true,
    "sourceMap": true,
    "composite": true,
    "strict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "paths": {
      "@daita/common": ["../../common/src/index.ts"],
      "@daita/common/*": ["../../common/src/*"],
      "@daita/relational": ["../../relational/src/index.ts"],
      "@daita/relational/*": ["../../relational/src/*"],
      "@daita/orm": ["../../orm/src/index.ts"],
      "@daita/orm/*": ["../../orm/src/*"],
      "@daita/node": ["../../node/src/index.ts"],
      "@daita/node/*": ["../../node/src/*"],
    },
  },
  "references": [{
    "path": "../common"
  }, {
    "path": "../relational"
  }, {
    "path": "../orm"
  }, {
    "path": "../node"
  }],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
