{
  "compilerOptions": {
    "baseUrl": "src",
    "rootDir": "src",
    "module": "CommonJS",
    "target": "ES2015",
    "outDir": "dist",
    "tsBuildInfoFile": "dist/tsconfig.tsbuildinfo",
    "lib": [
      "es2016",
      "es2019",
      "ES2020",
      "dom"
    ],
    "moduleResolution": "node",
    "declaration": true,
    "declarationMap": false,
    "esModuleInterop": true,
    "incremental": true,
    "sourceMap": false,
    "composite": true,
    "strict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "paths": {
      "@daita/common": ["../../common/src/index.ts"],
      "@daita/common/*": ["../../common/src/*"],
      "@daita/http-adapter": ["../../http-adapter/src/index.ts"],
      "@daita/http-adapter/*": ["../../http-adapter/src/*"],
      "@daita/pg-adapter": ["../../pg-adapter/src/index.ts"],
      "@daita/pg-adapter/*": ["../../pg-adapter/src/*"],
      "@daita/sqlite-adapter": ["../../sqlite-adapter/src/index.ts"],
      "@daita/sqlite-adapter/*": ["../../sqlite-adapter/src/*"],
      "@daita/create": ["../../create/src/index.ts"],
      "@daita/create/*": ["../../create/src/*"],
      "@daita/auth-server": ["../../auth-server/src/index.ts"],
      "@daita/auth-server/*": ["../../auth-server/src/*"],
      "@daita/http-server": ["../../http-server/src/index.ts"],
      "@daita/http-server/*": ["../../http-server/src/*"],
      "@daita/relational": ["../../relational/src/index.ts"],
      "@daita/relational/*": ["../../relational/src/*"],
      "@daita/node": ["../../node/src/index.ts"],
      "@daita/node/*": ["../../node/src/*"],
      "@daita/orm": ["../../orm/src/index.ts"],
      "@daita/orm/*": ["../../orm/src/*"],
      "@daita/http-server-common": ["../../http-server-common/src/index.ts"],
      "@daita/http-server-common/*": ["../../http-server-common/src/*"],
    }
  },
  "references": [{
    "path": "../common"
  }, {
    "path": "../http-adapter"
  }, {
    "path": "../sqlite-adapter"
  }, {
    "path": "../pg-adapter"
  }, {
    "path": "../orm"
  }, {
    "path": "../create"
  }, {
    "path": "../node"
  }, {
    "path": "../auth-server"
  }, {
    "path": "../relational"
  }, {
    "path": "../http-server"
  }, {
    "path": "../http-server-common"
  }],
  "exclude": [
    "node_modules",
    "dist",
    "src/**/*.spec.ts"
  ]
}
