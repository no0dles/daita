{
  "compilerOptions": {
    "baseUrl": "src",
    "rootDir": "src",
    "module": "CommonJS",
    "target": "ES2015",
    "outDir": "dist",
    "tsBuildInfoFile": "dist/tsconfig.tsbuildinfo",
    "lib": [
      "es2016",
      "es2019",
      "ES2020",
      "dom"
    ],
    "moduleResolution": "node",
    "declaration": true,
    "declarationMap": false,
    "esModuleInterop": true,
    "incremental": true,
    "sourceMap": true,
    "composite": true,
    "strict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "paths": {
      "@daita/common": ["../../../packages/common/src/index.ts"],
      "@daita/common/*": ["../../../packages/common/src/*"],
      "@daita/sqlite-adapter": ["../../../packages/sqlite-adapter/src/index.ts"],
      "@daita/sqlite-adapter/*": ["../../../packages/sqlite-adapter/src/*"],
      "@daita/mariadb-adapter": ["../../../packages/mariadb-adapter/src/index.ts"],
      "@daita/mariadb-adapter/*": ["../../../packages/mariadb-adapter/src/*"],
      "@daita/pg-adapter": ["../../../packages/pg-adapter/src/index.ts"],
      "@daita/pg-adapter/*": ["../../../packages/pg-adapter/src/*"],
      "@daita/relational": ["../../../packages/relational/src/index.ts"],
      "@daita/relational/*": ["../../../packages/relational/src/*"],
      "@daita/testing": ["../../../tooling/testing/src/index.ts"],
      "@daita/testing/*": ["../../../tooling/testing/src/*"],
      "@daita/orm": ["../../../packages/orm/src/index.ts"],
      "@daita/orm/*": ["../../../packages/orm/src/*"]
    }
  },
  "references": [{
    "path": "../../packages/iwent"
  }, {
    "path": "../../packages/relational"
  },  {
    "path": "../../packages/sqlite-adapter"
  }, {
    "path": "../../packages/pg-adapter"
  }, {
    "path": "../../packages/mariadb-adapter"
  }, {
    "path": "../../tooling/testing"
  }, {
    "path": "../../packages/orm"
  }, {
    "path": "../../packages/common"
  }],
  "exclude": [
    "node_modules",
    "dist",
    "jest.config.ts"
  ]
}
