{
  "compilerOptions": {
    "baseUrl": "src",
    "rootDir": "src",
    "module": "CommonJS",
    "target": "ES2015",
    "outDir": "dist",
    "tsBuildInfoFile": "dist/tsconfig.tsbuildinfo",
    "lib": [
      "es2016",
      "es2019",
      "ES2020"
    ],
    "moduleResolution": "node",
    "declaration": true,
    "declarationMap": false,
    "esModuleInterop": true,
    "incremental": true,
    "sourceMap": true,
    "composite": true,
    "strict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "paths": {
      "@daita/common": ["../../../packages/common/src/index.ts"],
      "@daita/common/*": ["../../../packages/common/src/*"],
      "@daita/relational": ["../../../packages/relational/src/index.ts"],
      "@daita/relational/*": ["../../../packages/relational/src/*"],
      "@daita/orm": ["../../../packages/orm/src/index.ts"],
      "@daita/orm/*": ["../../../packages/orm/src/*"],
      "@daita/node": ["../../../packages/node/src/index.ts"],
      "@daita/node/*": ["../../../packages/node/src/*"]
    }
  },
  "references": [{
    "path": "../../packages/common"
  }, {
    "path": "../../packages/relational"
  }, {
    "path": "../../packages/orm"
  }, {
    "path": "../../packages/node"
  }],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
