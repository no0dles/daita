{
  "compilerOptions": {
    "baseUrl": "src",
    "rootDir": "src",
    "module": "CommonJS",
    "target": "ES2015",
    "outDir": "dist",
    "tsBuildInfoFile": "dist/tsconfig.tsbuildinfo",
    "lib": [
      "es2016",
      "es2019",
      "ES2020",
      "dom"
    ],
    "moduleResolution": "node",
    "declaration": true,
    "declarationMap": false,
    "esModuleInterop": true,
    "incremental": true,
    "sourceMap": true,
    "composite": true,
    "strict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "paths": {
      "@daita/common": ["../../../packages/common/src/index.ts"],
      "@daita/common/*": ["../../../packages/common/src/*"],
      "@daita/node": ["../../../packages/node/src/index.ts"],
      "@daita/node/*": ["../../../packages/node/src/*"],
      "@daita/testing": ["../../testing/src/index.ts"],
      "@daita/testing/*": ["../../testing/src/*"]
    }
  },
  "references": [{
    "path": "../../packages/common"
  }, {
    "path": "../../packages/node"
  }, {
    "path": "../testing"
  }],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
