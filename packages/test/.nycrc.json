{
  "extension": [".ts"],
  "include": [
    "**/node_modules/@daita/**",
    "node_modules/@daita/**",
    "../../packages/**",
    "../../node_modules/@daita/**"
  ],
  "excludeNodeModules": false,
  "exclude": [
    "**/*.d.ts",
    "**/*.spec.ts",
    "coverage/**",
    "node_modules/@daita/**/node_modules/**"
  ],
  "reporter": [
    "json",
    "html",
    "text-summary"
  ],
  "all": true
}
