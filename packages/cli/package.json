{
  "name": "@daita/cli",
  "version": "0.1.0-alpha.0",
  "author": "Pascal Bertschi @no0dles",
  "bin": {
    "dc": "./bin/run"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": "https://github.com/no0dles/daita/issues",
  "dependencies": {
    "@daita/core": "^0.1.0-alpha.0",
    "@daita/web": "^0.1.0-alpha.0",
    "chokidar": "^3.3.1",
    "cli-ux": "^5.4.1",
    "config": "^3.2.4",
    "inquirer": "^7.0.1",
    "listr": "^0.14.3",
    "nomnoml": "^0.6.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@types/chokidar": "^2.1.3",
    "@types/config": "^0.0.36",
    "@types/inquirer": "^6.5.0",
    "@types/listr": "^0.14.2",
    "typescript": "^3.6.4",
    "tslint": "^5.20.1",
    "@daita/tslint": "0.1.0-alpha.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/no0dles/daita",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "oclif": {
    "commands": "./dist/commands",
    "bin": "dc",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "build": "tsc",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && rm -f tsconfig.tsbuildinfo && tsc -b && oclif-dev manifest && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md",
    "lint": "tslint src/**/*.ts",
    "prettier": "prettier 'src/**/*.ts' --write"
  },
  "types": "dist/index.d.ts"
}
