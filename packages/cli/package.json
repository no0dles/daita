    "config": "^3.2.4",
    "inquirer": "^7.0.1",
    "nomnoml": "^0.6.2",
    "cli-ux": "^5.4.6",
    "commander": "^5.1.0"
  },
  "devDependencies": {
    "@daita/pg-adapter": "file:../pg-adapter",
    "@daita/http-adapter": "file:../http/http-adapter",
